#: src/common/outbound.c:1832
#, c-format
msgid "Unknown arg '%s' ignored."
msgstr "Невідомий аргумент '%s' проігноровано."

#: src/common/outbound.c:2818
msgid "No such plugin found.\n"
msgstr "На знайдено такого модуля.\n"

#: src/common/outbound.c:2823 src/fe-gtk/plugingui.c:160
msgid "That plugin is refusing to unload.\n"
msgstr "Не вдається вивантажити модуль.\n"

#: src/common/outbound.c:3066
msgid "ADDBUTTON <name> <action>, adds a button under the user-list"
msgstr "ADDBUTTON <ім'я> <дія>, додає кнопку над списком користувачів"

#: src/common/outbound.c:3068
msgid "ALLCHAN <cmd>, sends a command to all channels you're in"
msgstr ""
"ALLCHAN <команда>, надсилає команду до всіх каналів, з якими ви з'єднані"

#: src/common/outbound.c:3070
msgid "ALLSERV <cmd>, sends a command to all servers you're in"
msgstr "ALLSERV <cmd>, надсилає команду до всіх серверів, з якими ви з'єднані"

#: src/common/outbound.c:3071
msgid "AWAY [<reason>], sets you away"
msgstr "AWAY [<підстава>], встановлює ваш стан як \"відсутній\""

#: src/common/outbound.c:3073
#, fuzzy
msgid ""
"BAN <mask> [<masktype>], bans everyone matching the mask from the current "
"channel. If they are already on the channel this doesn't kick them (needs "
"chanop)"
msgstr ""
"BAN <маска> [<тип>], забороняє доступ до каналу всім, хто відповідає "
"вказаній масці. Якщо користувачі вже з'єднані з каналом, вони не будуть "
"відключені (для цього потрібно бути оператором каналу)"

#: src/common/outbound.c:3075
msgid "CLEAR, Clears the current text window"
msgstr "CLEAR, очищує поточне текстове вікно"

#: src/common/outbound.c:3076
msgid "CLOSE, Closes the current window/tab"
msgstr "CLOSE, закриває поточне вікно/вкладку"

#: src/common/outbound.c:3078
msgid "COUNTRY <code>, finds a country code, eg: au = australia"
msgstr "COUNTRY <код>, знаходить код країни, наприклад: au = australia"

#: src/common/outbound.c:3080
msgid ""
"CTCP <nick> <message>, send the CTCP message to nick, common messages are "
"VERSION and USERINFO"
msgstr ""
"CTCP <прізвисько> <повідомлення>, надсилає повідомлення CTCP до користувача "
"з вказаним ім'ям, звичайними повідомленнями є VERSION та USERINFO"

#: src/common/outbound.c:3082
msgid "CYCLE, parts current channel and immediately rejoins"
msgstr "CYCLE, вийти з поточного каналу та одразу з'єднатись знову"

#: src/common/outbound.c:3083
msgid ""
"\n"
"DCC GET <nick>                     - accept an offered file\n"
"DCC SEND [-maxcps=#] <nick> [file] - send a file to someone\n"
"DCC LIST                           - show DCC list\n"
"DCC CHAT <nick>                    - offer DCC CHAT to someone\n"
"DCC CLOSE <type> <nick> <file>         example:\n"
"         /dcc close send johnsmith file.tar.gz"
msgstr ""
"\n"
"DCC GET <прізвисько>                     - прийняти запропонований файл\n"
"DCC SEND [-maxcps=#] <прізвисько> [файл] - надіслати файл отримувачу\n"
"DCC LIST                                 - показати список DCC\n"
"DCC CHAT <прізвисько>                    - запропонувати розмову через DCC\n"
"DCC CLOSE <тип> <прізвисько> <файл>        приклад:\n"
"         /dcc close send johnsmith file.tar.gz"

#: src/common/outbound.c:3092
msgid ""
"DEHOP <nick>, removes chanhalf-op status from the nick on the current "
"channel (needs chanop)"
msgstr ""
"DEHOP <прізвисько>, скасувати стан напівоператор каналу (chanhalf-op) у "
"особи на поточному каналі (потребує прав адміністратора каналу)"

#: src/common/outbound.c:3094
msgid "DELBUTTON <name>, deletes a button from under the user-list"
msgstr "DELBUTTON <ім'я>, видаляє кнопку зі списку користувачів"

#: src/common/outbound.c:3096
msgid ""
"DEOP <nick>, removes chanop status from the nick on the current channel "
"(needs chanop)"
msgstr ""
"DEOP <прізвисько>, скасовує права оператора (chanop) у вказаної особи на "
"поточному каналі (потребує прав адміністратора каналу)"

#: src/common/outbound.c:3098
msgid ""
"DEVOICE <nick>, removes voice status from the nick on the current channel "
"(needs chanop)"
msgstr ""
"DEVOICE <прізвисько>, скасовує право голосу у вказаної особи на поточному "
"каналі (потребує прав адміністратора каналу)"

#: src/common/outbound.c:3099
msgid "DISCON, Disconnects from server"
msgstr "DISCON, від'єднатись від сервера"

#: src/common/outbound.c:3100
msgid "DNS <nick|host|ip>, Finds a users IP number"
msgstr "DNS <прізвисько|вузол|ip>, знайти IP адрес користувача"

#: src/common/outbound.c:3101
msgid "ECHO <text>, Prints text locally"
msgstr "ECHO <текст>, вивести текст на локальній машині"

#: src/common/outbound.c:3104
msgid ""
"EXEC [-o] <command>, runs the command. If -o flag is used then output is "
"sent to current channel, else is printed to current text box"
msgstr ""
"EXEC [-o] <команда>, виконує команду. Якщо використано параметр -o , вивід "
"буде надіслано до поточного каналу, інакше він буде виведений в поточному "
"вікні"

#: src/common/outbound.c:3106
msgid "EXECCONT, sends the process SIGCONT"
msgstr "EXECCONT, надсилає процесу сигнал SIGCONT"

#: src/common/outbound.c:3109
msgid ""
"EXECKILL [-9], kills a running exec in the current session. If -9 is given "
"the process is SIGKILL'ed"
msgstr ""
"EXECKILL [-9], перериває виконання процесу в поточному сеансу. Якщо задано "
"параметр -9 процес буде припинено за допомогою SIGKILL"

#: src/common/outbound.c:3111
msgid "EXECSTOP, sends the process SIGSTOP"
msgstr "EXECSTOP, надсилає процесу сигнал SIGSTOP"

#: src/common/outbound.c:3112
msgid "EXECWRITE, sends data to the processes stdin"
msgstr "EXECWRITE, надсилає дані в стандартний ввід процесу"

#: src/common/outbound.c:3115
msgid "EXEMPT <mask> [masktype], sets a exempt"
msgstr ""

#: src/common/outbound.c:3117
msgid "FLUSHQ, flushes the current server's send queue"
msgstr "FLUSHQ, скидає поточну чергу відправлення на поточному сервері"

#: src/common/outbound.c:3119
msgid "GATE <host> [<port>], proxies through a host, port defaults to 23"
msgstr "GATE <вузол> [<порт>], під'єднується через проксі, типовий порт 23"

#: src/common/outbound.c:3125
msgid "HOP <nick>, gives chanhalf-op status to the nick (needs chanop)"
msgstr ""
"HOP <прізвисько>, надає стан напівоператора (chanhalf-op) вказаній особі "
"(потребує прав оператора)"

#: src/common/outbound.c:3127
msgid ""
"IGNORE <mask> <types..> <options..>\n"
"    mask - host mask to ignore, eg: *!*@*.aol.com\n"
"    types - types of data to ignore, one or all of:\n"
"            PRIV, CHAN, NOTI, CTCP, DCC, INVI, ALL\n"
"    options - NOSAVE, QUIET"
msgstr ""
"IGNORE <маска> <типи..> <параметри..>\n"
"    маска     - маска ігнорованого вузла, наприклад: *!*@*.aol.com\n"
"    типи      - типи ігнорованих даних, однин або декілька з наступних:\n"
"                PRIV, CHAN, NOTI, CTCP, INVI, ALL\n"
"    параметри - NOSAVE, QUIET"

#: src/common/outbound.c:3133
msgid ""
"INVITE <nick> [<channel>], invites someone to a channel, by default the "
"current channel (needs chanop)"
msgstr ""
"INVITE <прізвисько> [<канал>], запрошує когось до каналу, типово до "
"поточного каналу (потребує прав оператора каналу)"

#: src/common/outbound.c:3134
msgid "JOIN <channel>, joins the channel"
msgstr "JOIN <канал>, під'єднатися до вказаного каналу"

#: src/common/outbound.c:3136
msgid "KICK <nick>, kicks the nick from the current channel (needs chanop)"
msgstr ""
"KICK <прізвисько>, вилучити особу з поточного каналу (потребує прав "
"оператора)"

#: src/common/outbound.c:3138
msgid ""
"KICKBAN <nick>, bans then kicks the nick from the current channel (needs "
"chanop)"
msgstr ""
"KICKBAN <прізвисько>, забороняє а потім вилучає особу з поточного каналу "
"(потребує прав оператора каналу)"

#: src/common/outbound.c:3140
msgid "LAGCHECK, forces a new lag check"
msgstr "LAGCHECK, виконує перевірку перевірку сигналу"

#: src/common/outbound.c:3142
msgid "LASTLOG <string>, searches for a string in the buffer"
msgstr "LASTLOG <рядок>, шукає рядок в буфері"

#: src/common/outbound.c:3144
msgid "LOAD <file>, loads a plugin or script"
msgstr "LOAD <файл>, завантажує модуль або сценарій"

#: src/common/outbound.c:3146
msgid ""
"MDEHOP, Mass deop's all chanhalf-ops in the current channel (needs chanop)"
msgstr ""
"MDEHOP, скасовує стан напівоператора з усіх користувачів в поточному каналі "
"(потребує прав оператора)"

#: src/common/outbound.c:3148
msgid "MDEOP, Mass deop's all chanops in the current channel (needs chanop)"
msgstr ""
"MDEOP, скасовує стан оператора з усіх користувачів каналу (потребує прав "
"оператора)"

#: src/common/outbound.c:3150
msgid ""
"ME <action>, sends the action to the current channel (actions are written in "
"the 3rd person, like /me jumps)"
msgstr ""
"ME <дія>, надсилає подію до поточного каналу (події пишеться від третьої "
"особи,наприклад /me стрибає)"

#: src/common/outbound.c:3152
msgid ""
"MKICK, Mass kicks everyone except you in the current channel (needs chanop)"
msgstr ""
"MKICK, вилучає усіх окрім вас з поточного каналу (потребує прав оператора)"

#: src/common/outbound.c:3155
msgid "MOP, Mass op's all users in the current channel (needs chanop)"
msgstr ""
"MDEOP, встановлює всім користувачам поточного каналу права оператора "
"(потребує прав оператора)"

#: src/common/outbound.c:3156
msgid "MSG <nick> <message>, sends a private message"
msgstr "MSG <прізвисько> <повідомлення>, надсилає особисте повідомлення"

#: src/common/outbound.c:3157
msgid "NAMES, Lists the nicks on the current channel"
msgstr "NAMES, виводить список усіх користувачів поточного каналу"

#: src/common/outbound.c:3158
msgid "NCTCP <nick> <message>, Sends a CTCP notice"
msgstr "NCTCP <прізвисько> <повідомлення>, надсилає CTCP повідомлення"

#: src/common/outbound.c:3159
msgid "NEWSERVER <hostname> [<port>]"
msgstr "NEWSERVER <ім'я вузла> [<порт>]"

#: src/common/outbound.c:3160
msgid "NICK <nickname>, sets your nick"
msgstr "NICK <прізвисько>, змінює ваше прізвисько"

#: src/common/outbound.c:3162
msgid ""
"NOTICE <nick/channel> <message>, sends a notice. Notices are a type of "
"message that should be auto reacted to"
msgstr ""
"NOTICE <прізвисько/канал> <повідомлення>, надсилає сповіщення. Сповіщення - "
"це тип повідомлень, на які слід автоматично реагувати"

#: src/common/outbound.c:3163
msgid "NOTIFY [<nick>], lists your notify list or adds someone to it"
msgstr ""
"NOTIFY [<прізвисько>], виводить ваш список сповіщень або додає до нього "
"когось"

#: src/common/outbound.c:3164
msgid "OP <nick>, gives chanop status to the nick (needs chanop)"
msgstr ""
"OP <прізвисько>, надає вказаній особі статус оператора каналу (потребує прав "
"оператора)"

#: src/common/outbound.c:3166
msgid ""
"PART [<channel>] [<reason>], leaves the channel, by default the current one"
msgstr "PART [<канал>] [<привід>], вийти з каналу, типово - з поточного каналу"

#: src/common/outbound.c:3167
msgid "PING <nick | channel>, CTCP pings nick or channel"
msgstr "PING <прізвисько | канал>, CTCP пінгує особу або канал"

#: src/common/outbound.c:3168
msgid "QUERY <nick>, opens up a new privmsg window to someone"
msgstr ""
"QUERY <прізвисько>, відкриває нове вікно приватних повідомлень до вказаної "
"особи"

#: src/common/outbound.c:3169
msgid "QUIT [<reason>], disconnects from the current server"
msgstr "QUIT [<підстава>], від'єднання від поточного серверу"

#: src/common/outbound.c:3170
msgid "QUOTE <text>, sends the text in raw form to the server"
msgstr "QUOTE <текст>, надсилає текст в необробленій формі до серверу"

#: src/common/outbound.c:3173
msgid ""
"RECONNECT [-ssl] [<host>] [<port>] [<password>], Can be called just as /"
"RECONNECT to reconnect to the current server or with /RECONNECT ALL to "
"reconnect to all the open servers"
msgstr ""
"RECONNECT [-ssl] [<вузол>] [<порт>] [<пароль>], може бути викликано просто "
"як/RECONNECT ,щоб переприєднатися до поточного серверу або /RECONNECT ALL ,"
"щоб переприєднатися до усіх відкритих серверів"

#: src/common/outbound.c:3176
msgid ""
"RECONNECT [<host>] [<port>] [<password>], Can be called just as /RECONNECT "
"to reconnect to the current server or with /RECONNECT ALL to reconnect to "
"all the open servers"
msgstr ""
"RECONNECT [-ssl] [<вузол>] [<порт>] [<пароль>], може бути викликано просто "
"як/RECONNECT ,щоб переприєднатися до поточного серверу або /RECONNECT ALL ,"
"переприєднатися до усіх відкритих серверів"

#: src/common/outbound.c:3179
msgid ""
"RECV <text>, send raw data to xchat, as if it was received from the irc "
"server"
msgstr ""
"RECV <текст>, надсилає дані в необробленому форматі до xchat, як відправлені "
"з irc сервера"

#: src/common/outbound.c:3180
msgid "RPING <server> [server], ping between servers"
msgstr ""

#: src/common/outbound.c:3181
msgid "SAY <text>, sends the text to the object in the current window"
msgstr "SAY <текст>, надсилає текст до об'єкту в поточному вікні"

#: src/common/outbound.c:3184
msgid "SERVCHAN [-ssl] <host> <port> <channel>, connects and joins a channel"
msgstr ""
"SERVCHAN [-ssl] <вузол> <порт> <канал>, встановити з'єднання з сервером та "
"під'єднатись до каналу"

#: src/common/outbound.c:3187
msgid "SERVCHAN <host> <port> <channel>, connects and joins a channel"
msgstr ""
"SERVCHAN <вузол> <порт> <канал>, встановити з'єднання з сервером та "
"під'єднатись до каналу"

#: src/common/outbound.c:3192
msgid ""
"SERVER [-ssl] <host> [<port>] [<password>], connects to a server, the "
"default port is 6667 for normal connections, and 9999 for ssl connections"
msgstr ""
"SERVCHAN <вузол> <порт> <канал>, встановлює з'єднання сервером.  Типовий "
"порт для звичайних з'єднань 6667 та  9999 для ssl з'єднань"

#: src/common/outbound.c:3195
msgid ""
"SERVER <host> [<port>] [<password>], connects to a server, the default port "
"is 6667"
msgstr ""
"SERVER <вузол> [<порт>] [<пароль>], з'єднує з сервером, типовий порт 6667"

#: src/common/outbound.c:3197
#, fuzzy
msgid "SET [-quiet] <variable> [<value>]"
msgstr "SET <змінна> [<значення>]"

#: src/common/outbound.c:3198
msgid "SETCURSOR [-|+]<position>"
msgstr ""

#: src/common/outbound.c:3202
msgid ""
"TOPIC [<topic>], sets the topic if one is given, else shows the current topic"
msgstr ""
"TOPIC [<тема>], встановлює тему, якщо вона вказана. У іншому випадку "
"відображає поточну тему"

#: src/common/outbound.c:3203
msgid "UNBAN <mask> [<mask>...], unbans the specified masks."
msgstr ""
