#: src/common/outbound.c:31
msgid "No channel joined. Try /join #<channel>\n"
msgstr "Niste na nobenem kanalu. Poskusite /join #<kanal>\n"

#: src/common/outbound.c:37
msgid "Not connected. Try /server <host> [<port>]\n"
msgstr "Niste povezani. Poskusite /server <gostitelj> [<vrata>]\n"

#: src/common/outbound.c:1446
msgid "I need /bin/sh to run!\n"
msgstr "Potrebujem /bin/sh za zagon!\n"

#: src/common/outbound.c:1716
msgid ""
"\n"
"Commands Available:\n"
"\n"
"  "
msgstr ""
"\n"
"Ukazi, ki so na voljo:\n"
"\n"
"  "

#: src/common/outbound.c:1749
msgid ""
"\n"
"\n"
"Type /HELP <command> for more information, or /HELP -l\n"
"\n"
msgstr ""
"\n"
"\n"
"Za več informacij uporabite /HELP <ukaz> ali /HELP -l\n"
"\n"

#: src/common/outbound.c:1750
msgid ""
"User defined commands:\n"
"\n"
"  "
msgstr ""
"Ukazi, določeni s strani uporabnika:\n"
"\n"
"  "

#: src/common/outbound.c:1846
#, c-format
msgid "Unknown arg '%s' ignored."
msgstr "Neznan argument '%s' spregledan."

#: src/common/outbound.c:2789
msgid "No such plugin found.\n"
msgstr "Ta dodatek ne obstaja.\n"

#: src/common/outbound.c:2794 src/fe-gtk/plugingui.c:162
msgid "That plugin is refusing to unload.\n"
msgstr "Ta plugin zavrača odstranitev.\n"

#: src/common/outbound.c:3054
msgid "ADDBUTTON <name> <action>, adds a button under the user-list"
msgstr "ADDBUTTON <ime> <dejanje>, doda gumb v seznam uporabnikov"

#: src/common/outbound.c:3056
msgid "ALLCHAN <cmd>, sends a command to all channels you're in"
msgstr "ALLCHAN <ukaz>, izvede ukaz na vseh kanalih, na katerih ste prisotni"

#: src/common/outbound.c:3058
msgid "ALLSERV <cmd>, sends a command to all servers you're in"
msgstr "ALLSERV <ukaz>, pošlje ukaz vsem strežnikom, na katerih ste"

#: src/common/outbound.c:3059
msgid "AWAY [<reason>], sets you away"
msgstr "AWAY [<razlog>], nastavi vašo odsotnost"

#: src/common/outbound.c:3061
msgid ""
"BAN <mask> [<bantype>], bans everyone matching the mask from the current "
"channel. If they are already on the channel this doesn't kick them (needs "
"chanop)"
msgstr ""
"BAN <maska> [<vrsta>], prepove dostop do trenutnega kanala vsem, ki "
"ustrezajo maski. Če so že na kanalu, jih ta ukaz ne brcne iz kanala "
"(potrebuje status op)"

#: src/common/outbound.c:3063
msgid "CLEAR, Clears the current text window"
msgstr "CLEAR, počisti besedilo v trenutnem oknu"

#: src/common/outbound.c:3064
msgid "CLOSE, Closes the current window/tab"
msgstr "CLOSE, zapre trenutno okno/zavihek"

#: src/common/outbound.c:3066
msgid "COUNTRY <code>, finds a country code, eg: au = australia"
msgstr "COUNTRY <koda>, poišče ime države, npr: si = Slovenija"

#: src/common/outbound.c:3068
msgid ""
"CTCP <nick> <message>, send the CTCP message to nick, common messages are "
"VERSION and USERINFO"
msgstr ""
"CTCP <vzdevek> <sporočilo>, pošlje sporočilo CTCP vzdevku, pogosti sporočili "
"sta VERSION in USERINFO"

#: src/common/outbound.c:3070
msgid "CYCLE, parts current channel and immediately rejoins"
msgstr "CYCLE, zapusti trenutni kanal in se takoj vrne nazaj"

#: src/common/outbound.c:3072
msgid ""
"\n"
"DCC GET <nick>                     - accept an offered file\n"
"DCC SEND [-maxcps=#] <nick> [file] - send a file to someone\n"
"DCC LIST                           - show DCC list\n"
"DCC CHAT <nick>                    - offer DCC CHAT to someone\n"
"DCC CLOSE <type> <nick> <file>         example:\n"
"         /dcc close send johnsmith file.tar.gz"
msgstr ""
"\n"
"DCC GET <vzdevek>              - prejme ponujeno datoteko\n"
"DCC SEND [-maxcps=#] <vzdevek> [datoteka]  - pošlje datoteko vzdevku\n"
"DCC LIST                       - pokaže DCC seznam\n"
"DCC CHAT <vzdevek>             - vzdevku ponudi DCC CHAT\n"
"DCC CLOSE <vrsta> <vzdevek> <datoteka>         primer:\n"
"          /dcc close send janeznovak datoteka.tar.gz"

#: src/common/outbound.c:3081
msgid ""
"DEHOP <nick>, removes chanhalf-op status from the nick on the current "
"channel (needs chanop)"
msgstr ""
"DEHOP <vzdevek>, odstrani status polovični-op vzdevku (potrebuje status op)"

#: src/common/outbound.c:3083
msgid "DELBUTTON <name>, deletes a button from under the user-list"
msgstr "DELBUTTON <ime>, izbriše gumb s seznama uporabnikov"

#: src/common/outbound.c:3085
msgid ""
"DEOP <nick>, removes chanop status from the nick on the current channel "
"(needs chanop)"
msgstr "DEOP <vzdevek>, odstrani status op vzdevku (potrebuje status op)"

#: src/common/outbound.c:3087
msgid ""
"DEVOICE <nick>, removes voice status from the nick on the current channel "
"(needs chanop)"
msgstr ""
"DEVOICE <vzdevek>, odstrai glasovni privilegij vzdevku (potrebuje status op)"

#: src/common/outbound.c:3088
msgid "DISCON, Disconnects from server"
msgstr "DISCON, prekine povezavo s strežnikom"

#: src/common/outbound.c:3089
msgid "DNS <nick|host|ip>, Finds a users IP number"
msgstr "DNS <vzdevek|gostitelj|ip>, poišče uporabnikovo številko IP"

#: src/common/outbound.c:3090
msgid "ECHO <text>, Prints text locally"
msgstr "ECHO <besedilio>, lokalno izpiše besedilo"

#: src/common/outbound.c:3093
msgid ""
"EXEC [-o] <command>, runs the command. If -o flag is used then output is "
"sent to current channel, else is printed to current text box"
msgstr ""
"EXEC [-o] <ukaz>, izvede ukaz. Če uporabite stikalo -o, je rezultat izpisan "
"na trenutnem kanalu, v nasprotnem primeru pa v trenutnem besedilnem polju"

#: src/common/outbound.c:3095
msgid "EXECCONT, sends the process SIGCONT"
msgstr "EXECCONT, procesu pošlje SIGCONT"

#: src/common/outbound.c:3098
msgid ""
"EXECKILL [-9], kills a running exec in the current session. If -9 is given "
"the process is SIGKILL'ed"
msgstr ""
"EXECKILL [-9], ubije delujoči proces v trenutni seji. Če uporabite stikalo -"
"9, je procesu poslan SIGKILL"

#: src/common/outbound.c:3100
msgid "EXECSTOP, sends the process SIGSTOP"
msgstr "EXECSTOP, procesu pošlje SIGSTOP"

#: src/common/outbound.c:3101
msgid "EXECWRITE, sends data to the processes stdin"
msgstr "EXECWRITE, pošlje podatke vhodnemu toku procesa"

#: src/common/outbound.c:3105
msgid "FLUSHQ, flushes the current server's send queue"
msgstr "FULSHQ, odpošlje čakajočo vrsto sporočil za strežnik"

#: src/common/outbound.c:3107
msgid "GATE <host> [<port>], proxies through a host, port defaults to 23"
msgstr ""
"GATE <gostitelj> [<vrata>], preusmeri skozi gostitelja, privzeta vrata so 23"

#: src/common/outbound.c:3113
msgid "HOP <nick>, gives chanhalf-op status to the nick (needs chanop)"
msgstr ""
"HOP <vzdevek>, vzdevku dodeli status polovični-op (potrebuje status op)"

#: src/common/outbound.c:3115
msgid ""
"IGNORE <mask> <types..> <options..>\n"
"    mask - host mask to ignore, eg: *!*@*.aol.com\n"
"    types - types of data to ignore, one or all of:\n"
"            PRIV, CHAN, NOTI, CTCP, DCC, INVI, ALL\n"
"    options - NOSAVE, QUIET"
msgstr ""
"IGNORE <maska> <vrste..> <možnosti..>\n"
"    maska - maska gostitelja, npr.: *!*@*.siol.net\n"
"    vrste - vrste podatkov za preziranje, ena ali vse od:\n"
"           PRIV, CHAN, NOTI, CTCP, DCC, INVI, ALL\n"
"    možnosti - NOSAVE, QUIET"

#: src/common/outbound.c:3121
msgid ""
"INVITE <nick> [<channel>], invites someone to a channel, by default the "
"current channel (needs chanop)"
msgstr ""
"INVITE <vzdevek> [<kanal>], povabi vzdevek na kanal, privzeto na trenutni "
"kanal (potrebuje status op)"

#: src/common/outbound.c:3122
msgid "JOIN <channel>, joins the channel"
msgstr "JOIN <kanal>, pridruži se kanalu"

#: src/common/outbound.c:3124
msgid "KICK <nick>, kicks the nick from the current channel (needs chanop)"
msgstr ""
"KICK <vzdevek>, brcne vzdevek iz trenutnega kanala (potrebuje status op)"

#: src/common/outbound.c:3126
msgid ""
"KICKBAN <nick>, bans then kicks the nick from the current channel (needs "
"chanop)"
msgstr ""
"KICKBAN <vzdevek>, nastavi prepoved in brcne osebo iz kanala (potrebuje "
"status op)"

#: src/common/outbound.c:3129
msgid "LAGCHECK, forces a new lag check"
msgstr "LAGCHECK, zažene preverjanje časovnega zamika"

#: src/common/outbound.c:3131
msgid "LASTLOG <string>, searches for a string in the buffer"
msgstr "LASTLOG <niz>, išče besedilni niz v oknu"

#: src/common/outbound.c:3133
msgid "LOAD <file>, loads a plugin or script"
msgstr "LOAD <datoteka>, naloži dodatek ali skripto"

#: src/common/outbound.c:3135
msgid ""
"MDEHOP, Mass deop's all chanhalf-ops in the current channel (needs chanop)"
msgstr ""
"MDEHOP, masovno odvzame vse polovične statuse op na trenutnem kanalu "
"(potrebuje status op)"

#: src/common/outbound.c:3137
msgid "MDEOP, Mass deop's all chanops in the current channel (needs chanop)"
msgstr ""
"MDEOP, množično odvzame status op na trenutnem kanalu (potrebuje status op)"

#: src/common/outbound.c:3139
msgid ""
"ME <action>, sends the action to the current channel (actions are written in "
"the 3rd person, like /me jumps)"
msgstr ""
"ME <dejanje>, pošlje dejanje v trenutno kanal (dejanja so izpisana v tretji "
"osebi, npr. /me skače)"

#: src/common/outbound.c:3141
msgid ""
"MKICK, Mass kicks everyone except you in the current channel (needs chanop)"
msgstr ""
"MKICK, množično brcne vse, razen vas, na trenutnem kanalu (potrebuje status "
"op)"

#: src/common/outbound.c:3144
msgid "MOP, Mass op's all users in the current channel (needs chanop)"
msgstr ""
"MDEOP, množično odvzame status op na trenutnem kanalu (potrebuje status op)"

#: src/common/outbound.c:3145
msgid "MSG <nick> <message>, sends a private message"
msgstr "MSG <vzdevek> <sporočilo>, pošlje zasebno sporočilo"

#: src/common/outbound.c:3147
msgid "NAMES, Lists the nicks on the current channel"
msgstr "NAMES, izpiše seznam vzdevkov na trenutnem kanalu"

#: src/common/outbound.c:3149
msgid "NCTCP <nick> <message>, Sends a CTCP notice"
msgstr "NCTCP <vzdevek> <sporočilo>, pošlje obvestilo CTCP"

#: src/common/outbound.c:3150
msgid "NEWSERVER <hostname> [<port>]"
msgstr "NEWSERVER <gostitelj> [<vrata>]"

#: src/common/outbound.c:3151
msgid "NICK <nickname>, sets your nick"
msgstr "NICK <vzdevek>, nastavi vaš vzdevek"

#: src/common/outbound.c:3153
msgid ""
"NOTICE <nick/channel> <message>, sends a notice. Notices are a type of "
"message that should be auto reacted to"
msgstr ""
"NOTICE <vzdevek/kanal> <sporočilo>, pošlje obvestilo. To je vrsta sporočila, "
"na katerega je možen samodejen odziv"

#: src/common/outbound.c:3155
msgid "NOTIFY [<nick>], lists your notify list or adds someone to it"
msgstr ""
"NOTIFY [<vzdevek>], doda vzdevek v vaš seznam za obveščanje ali pa ga izpiše"

#: src/common/outbound.c:3157
msgid "OP <nick>, gives chanop status to the nick (needs chanop)"
msgstr ""
"OP <vzdevek>, vzdevku podeli status operateja (op) kanala (potrebuje status "
"op)"

#: src/common/outbound.c:3159
msgid ""
"PART [<channel>] [<reason>], leaves the channel, by default the current one"
msgstr "PART [<kanal>] [<razlog>], zapusti kanal, privzeto trenutnega"

#: src/common/outbound.c:3161
msgid "PING <nick | channel>, CTCP pings nick or channel"
msgstr "PING <vzdevek | kanal>, pošlje CTCP ping vzdevku ali kanalu"

#: src/common/outbound.c:3163
msgid "QUERY <nick>, opens up a new privmsg window to someone"
msgstr "QUERY <vzdevek>, odpre novo okno za zasebni pogovor"

#: src/common/outbound.c:3165
msgid "QUIT [<reason>], disconnects from the current server"
msgstr "QUIT [<razlog]>, prekine povezavo s trenutnim strežnikom"

#: src/common/outbound.c:3167
msgid "QUOTE <text>, sends the text in raw form to the server"
msgstr "QUOTE <besedilo>, strežniku pošlje besedilo v surovi obliki"

#: src/common/outbound.c:3170
msgid ""
"RECONNECT [-ssl] [<host>] [<port>] [<password>], Can be called just as /"
"RECONNECT to reconnect to the current server or with /RECONNECT ALL to "
"reconnect to all the open servers"
msgstr ""
"RECONNECT [-ssl] [<gostitelj>] [<vrata>] [<geslo>], lahko ga uporabite samo "
"kot /RECONNECT za ponovitev povezave na trenutni strežnik ali kot /RECONNECT "
"ALL za ponovitev povezave na vse odprte strežnike"

#: src/common/outbound.c:3173
msgid ""
"RECONNECT [<host>] [<port>] [<password>], Can be called just as /RECONNECT "
"to reconnect to the current server or with /RECONNECT ALL to reconnect to "
"all the open servers"
msgstr ""
"RECONNECT [<gostitelj>] [<vrata>] [<geslo>], lahko ga uporabite samo kot /"
"RECONNECT za ponovitev povezave na trenutni strežnik ali kot /RECONNECT ALL "
"za ponovitev povezave na vse odprte strežnike"

#: src/common/outbound.c:3175
msgid ""
"RECV <text>, send raw data to xchat, as if it was received from the irc "
"server"
msgstr ""
"RECV <besedilo>, pošlje surove podatke xchatu, kot da bi bili sprejeti od "
"strežnika IRC"

#: src/common/outbound.c:3176
msgid "RPING <server> [server], ping between servers"
msgstr ""

#: src/common/outbound.c:3178
msgid "SAY <text>, sends the text to the object in the current window"
msgstr "SAY <besedilo>, pošlje besedilo predmetu v trenutnem oknu"

#: src/common/outbound.c:3181
msgid "SERVCHAN [-ssl] <host> <port> <channel>, connects and joins a channel"
msgstr ""
"SERVCHAN [-ssl] <gostitelj> <vrata> <kanal>, poveže se na strežnik in se "
"pridruži kanalu"

#: src/common/outbound.c:3184
msgid "SERVCHAN <host> <port> <channel>, connects and joins a channel"
msgstr ""
"SERVCHAN <gostitelj> <vrata> <kanal>, poveže se na strežnik in se pridruži "
"kanalu"

#: src/common/outbound.c:3189
msgid ""
"SERVER [-ssl] <host> [<port>] [<password>], connects to a server, the "
"default port is 6667 for normal connections, and 9999 for ssl connections"
msgstr ""
"SERVER [-ssl] <gostitelj> [<vrata>] [<geslo>], poveže se na strežnik, "
"privzeta vrata za navadne povezave so 6667 in 9999 za povezave SSL"

#: src/common/outbound.c:3192
msgid ""
"SERVER <host> [<port>] [<password>], connects to a server, the default port "
"is 6667"
msgstr ""
"SERVER <gostitelj> [<vrata>] [<geslo>], poveže se na strežnik, privzeta "
"vrata so 6667"

#: src/common/outbound.c:3194
#, fuzzy
msgid "SET [-quiet] <variable> [<value>]"
msgstr "SET <spremenljivka> [<vrednost>]"

#: src/common/outbound.c:3197
msgid ""
"TOPIC [<topic>], sets the topic if one is given, else shows the current topic"
msgstr ""
"TOPIC [<tema>], če je tema podana, jo nastavi, v nasprotnem primeru pokaže "
"trenutno"

#: src/common/outbound.c:3199
msgid "UNBAN <mask> [<mask>...], unbans the specified masks."
msgstr "UNBAN <maska> [<maska> ...], odstrani prepoved za določene maske."

#: src/common/outbound.c:3200
msgid "UNIGNORE <mask> [QUIET]"
msgstr "UNIGNORE <maska> [QUIET]"

#: src/common/outbound.c:3201
msgid "UNLOAD <name>, unloads a plugin or script"
msgstr "LOAD <datoteka>, prekine dodatek ali skripto"

#: src/common/outbound.c:3204
msgid "VOICE <nick>, gives voice status to someone (needs chanop)"
msgstr ""
"VOICE <vzdevek>, dodeli glasovni privilegij vzdevku (potrebuje status op)"

#: src/common/outbound.c:3206
msgid "WALLCHAN <message>, writes the message to all channels"
msgstr "WALLCHAN <sporočilo>, pošlje sporočilo na vse kanale"

#: src/common/outbound.c:3208
#, fuzzy
msgid ""
"WALLCHOPS <message>, sends the message to all chanops on the current channel"
msgstr "WALLCHOP <sporočilo>, pošlje sporočilo vsem operaterjem kanala"

#: src/common/outbound.c:3210
#, fuzzy
msgid ""
"WALLVOICES <message>, sends the message to all voiced users on the current "
"channel"
msgstr "WALLCHOP <sporočilo>, pošlje sporočilo vsem operaterjem kanala"

#: src/common/outbound.c:3255
#, c-format
msgid "Usage: %s\n"
msgstr "Uporaba:%s\n"

#: src/common/outbound.c:3260
msgid ""
"\n"
"No help available on that command.\n"
msgstr ""
"\n"
"Na to temo ni na voljo pomoči.\n"

#: src/common/outbound.c:3266
msgid "No such command.\n"
msgstr "Ta ukaz ne obstaja.\n"

#: src/common/outbound.c:3625
msgid "Bad arguments for user command.\n"
msgstr "Napačne opcije za uporabnikov ukaz.\n"

#: src/common/outbound.c:3787
msgid "Too many recursive usercommands, aborting."
msgstr "Preveč agresivnih ukazov, opuščam."

#: src/common/outbound.c:3845
msgid "Unknown Command. Try /help\n"
msgstr "Neznan ukaz. Poskusite /help\n"

